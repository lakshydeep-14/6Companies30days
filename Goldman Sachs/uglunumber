	ull getNthUglyNo(int n) {
	    // code here
	    set<long long int> s;
	    s.insert(1);
	    n--;
	    while(n--){
	        auto z=s.begin();
	        int x= *z;
	        s.erase(z);
	        s.insert(x*2);
	        s.insert(x*3);
	        s.insert(x*5);
	        
	    }
	    return *s.begin();
	}